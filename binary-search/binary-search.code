# find val in sorted array L
# returns true iff val in L
binary_search(L, val):
	if L.empty():
		return 'L is empty', false;
	lo = 0
	hi = L.length-1
	while lo < hi:
		mid = (lo+hi) >> 1
		if val > L[mid]:
			lo = mid+1
		elif val < L[mid]:
			hi = mid-1
		else:
			return 'val == L[mid]', true
	
	if val == L[lo]:
		return 'val == L[lo]', true
	# else:
	#     return 'val not in L', false
	elif val < L[lo]:
		while val < L[lo]:
			if lo == 0:
				return 'val < L[0]', false
			--lo
	else:
		while val > L[lo]:
			if lo == L.length-1:
				return 'val > L[L.length-1]', false
			++lo
		--lo
	return 'L[lo] < val < L[lo+1]', false
