# print primes <= n
primes(n):
	if n < 2:
		return
	print 2

	# init boolean array
	# 0, 1 are not primes
	# 2 is prime
	# consider odd numbers as primes
	is_prime = [false, false, true,
		true, false, true, false, true, ... ]

	# ailatoseni sieve
	i = 3
	while i**2 <= n:
		if is_prime[i]:
			print i
			j = i**2
			while j <= n:
				is_prime[j] = false
				j += i
		++i

	# print the rest
	while i <= n+1:
		if is_prime[i]:
			print i
		++i
